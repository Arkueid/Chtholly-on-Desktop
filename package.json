{
  "name": "chtholly-live2d",
  "version": "0.1.0",
  "description": "Chtholly Live2D for Desktop",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "start": "electron .",
    "electron:build": "electron-builder",
    "build": "rollup -c rollup.config.js -f iife | terser -c -m > waifu-tips.js",
